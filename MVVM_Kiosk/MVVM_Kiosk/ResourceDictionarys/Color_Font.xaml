<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >
    
    <!--#region Color-->
    <Color x:Key="Color_Black">#363636</Color>
    <Color x:Key="Color_White">#FFFFFF</Color>
    <Color x:Key="Color_LightGray">#F0EEED</Color>
    <Color x:Key="Color_WhiteSmoke">#F5F5F5</Color>
    <Color x:Key="Color_LightRed">#DC143C</Color>
    <Color x:Key="Color_Trans">Transparent</Color>
    
    <SolidColorBrush x:Key="Brush_Black" Color="{StaticResource Color_Black}"/>
    <SolidColorBrush x:Key="Brush_White" Color="{StaticResource Color_White}"/>
    <SolidColorBrush x:Key="Brush_LightGray" Color="{StaticResource Color_LightGray}"/>
    <SolidColorBrush x:Key="Brush_WhiteSmoke" Color="{StaticResource Color_WhiteSmoke}"/>
    <SolidColorBrush x:Key="Brush_LightRed" Color="{StaticResource Color_LightRed}"/>
    <SolidColorBrush x:Key="Brush_Trans" Color="{StaticResource Color_Trans}"/>
    <!--#endregion-->
   
    <!--#region Font-->
    <!--#endregion-->
    
</ResourceDictionary>